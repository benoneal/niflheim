{
  "name": "niflheim",
  "version": "1.0.0",
  "description": "Painlessly animate mounting/unmounting React components",
  "repository": "https://github.com/benoneal/niflheim",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "mkdir dist && babel src --out-dir dist",
    "size": "npm run prepare && bundle-size ./",
    "lint": "standard",
